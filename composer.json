{
    "name": "phprtc/http-router",
    "description": "PHPRTC Http Router",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmad Mustapha",
            "email": "ahmardy@outlook.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "nikic/fast-route": "^1.3",
        "opis/closure": "^3.6",
        "phprtc/contracts": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.4",
        "symfony/var-dumper": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "RTC\\Http\\Router\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RTC\\Http\\Router\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "phpstan analyse",
        "test": "phpunit"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://phprtc.repo.repman.io"
        }
    ]
}
